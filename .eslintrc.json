{
    "extends": "airbnb-base",
    "env": {
      "mocha": true
    },
         "plugins": [
      "mocha",
      "chai-friendly"
    ],
    "rules": {
      "mocha/no-exclusive-tests": "error",
      "no-unused-expressions": 0,
      "chai-friendly/no-unused-expressions": 2,
      "no-extra-bind":"off",
      "no-labels" :"off",
      "no-restricted-syntax":"off"
    }
}
