{
  "name": "spotify-wrapper-jswithtddcourse",
  "version": "2.0.1",
  "description": "A wrapper to work with the Spotify Web API",
  "main": "lib/index.js",
  "scripts": {
    "clear": "rimraf lib",
    "build": "npm run clear &&  ./node_modules/.bin/babel --out-dir lib src",
    "build:umd": "webpack --output-filename spotify-wrapper.umd.js",
    "build:umd:min": "webpack --output-filename spotify-wrapper.umd.min.js -p",
    "build:all": "npm run build && npm run build:umd && npm run build:umd:min",
    "lint": "eslint src/*.js",
    "prepush": "npm run lint",
    "test": "mocha tests/**/*.spec.js --require babel-register",
    "test:tdd": "mocha tests/**/*.spec.js --require babel-register --watch",
    "test:coverage": "nyc npm test",
    "coveralls": "npm run test:coverage && nyc report --reporter=text-lcov | coveralls"
  },
  "files": [
    "dist",
    "lib"
  ],
  "nyc": {
    "check-covarage": true,
    "functions": 80,
    "lines": 80,
    "reporter": [
      "text",
      "html"
    ],
    "exclude": [
      "tests/**"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Moraissz/spotify-wrapper.git"
  },
  "keywords": [
    "js",
    "tdd"
  ],
  "author": "Lucas Morais",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Moraissz/spotify-wrapper/issues"
  },
  "homepage": "https://github.com/Moraissz/spotify-wrapper#readme",
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-loader": "^7.1.4",
    "babel-preset-env": "^1.6.1",
    "babel-register": "^6.26.0",
    "chai": "^4.1.2",
    "coveralls": "^3.0.0",
    "eslint": "^4.19.1",
    "eslint-config-airbnb-base": "^12.1.0",
    "eslint-plugin-chai-friendly": "^0.4.1",
    "eslint-plugin-import": "^2.9.0",
    "eslint-plugin-mocha": "^4.12.1",
    "husky": "^0.15.0-rc.8",
    "mocha": "^5.0.4",
    "nyc": "^11.6.0",
    "rimraf": "^2.6.2",
    "sinon": "^4.4.8",
    "sinon-chai": "^3.0.0",
    "sinon-stub-promise": "^4.0.0"
  },
  "dependencies": {
    "babel-polyfill": "^6.26.0",
    "node-fetch": "^2.1.1",
    "webpack": "^2.4.0"
  },
  "directories": {
    "example": "examples",
    "lib": "lib",
    "test": "tests"
  }
}
